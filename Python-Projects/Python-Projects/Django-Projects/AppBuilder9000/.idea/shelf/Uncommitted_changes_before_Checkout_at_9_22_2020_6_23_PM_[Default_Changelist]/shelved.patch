+    margin: 0 0 20px 0;
+}
+ul li,
+ol li {
+    margin-bottom: 10px;
+}
+img {
+  max-width: 100%;
+}
+* {
+    box-sizing: border-box;
+    outline: 0;
+}
+.wrapper {
+  max-width: 1100px;
+  width: 100%;
+  margin: 0 auto;
+}
+.clear{clear:both;}
+/*HELPER CLASS END*/
+
+
+/* ===========================================
