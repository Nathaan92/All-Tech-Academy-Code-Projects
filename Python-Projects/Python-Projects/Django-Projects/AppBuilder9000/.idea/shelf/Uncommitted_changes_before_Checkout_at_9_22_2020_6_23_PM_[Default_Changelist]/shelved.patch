+{% extends 'RestaurantApp/RestaurantApp_base.html' %}
+
+{% load staticfiles %}
+
+{% block content %}
+
+<!-- Delete Modal, hidden until first delete is clicked -->
+<div class="modal fade" id="deleteModal" tabindex="-1" aria-labelledby="deleteModalLabel" aria-hidden="true">
+  <div class="modal-dialog modal-dialog-centered">
+    <div class="modal-content">
+      <div class="modal-header">
+        <h5 class="modal-title" id="deleteModalLabel">Confirm Delete</h5>
+        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
+          <span aria-hidden="true">&times;</span>
+        </button>
+      </div>
+      <div class="modal-body">
+        Are you sure you want to remove {{ restaurant.name|title }}?
+      </div>
+      <div class="modal-footer">
+          <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
+          <button type="submit" class="btn btn-primary" onclick="deleteForm()">Delete</button>
+      </div>                                       <!-- ^THIS LINE ABOVE^ is the important javascript delete function -->
+    </div>
+  </div>
+</div>
+
+
+
+
+<h1>Editing {{ restaurant.name|title }}</h1>
+
+<div>
+    <form method="POST">
+        {% csrf_token %}
+        <table class="addTable">
+        {{ form }}
+        </table>
+        <p>(Bubble options are not required)</p>
+        <input type="submit" value="Save Changes">
+        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#deleteModal">
+                Delete
+        </button>
+    </form>
+</div>
+
+
+
+    <!-- DELETE FUNCTIONALITY -->
+    <!-- this additional form is hidden and holds the url to start the delete function in views.py -->
+    <form id="hiddenForm" action="{% url 'RestaurantDelete' restaurant.pk %}" method="POST">
+        {% csrf_token %}
+    </form>
+    <!-- this is the javascript attached to the modal delete button so that the hidden form can actually be submitted -->
+    <script type="text/javascript">
+        function deleteForm() {document.getElementById("hiddenForm").submit();}
+    </script>
+
+
+{% endblock %}
