+header {
+  background: transparent;
+  width: 100%;
+  position: relative;
+}
+header.sticky {
+  position: fixed;
+  top: 0;
+  left: 0;
+  right: 0;
+  background: rgba(0,0,0,0.85);
+  transform: translateY(-200%);
+  transition: 0.2s;
+  z-index: 5;
+}
+body.scroll header.sticky {
+  transform: translateY(0);
+}
+.nav-wrapper {
+  max-width: 1000px;
+  margin: 0 auto;
+  position: relative;
+  padding: 1px 0;
+  display: flex;
+  justify-content: space-between;
+  align-items: center;
+}
+nav ul li {
+  display: inline-block;
+  padding: 0 10px;
+  margin-bottom: 0;
+}
+nav.nav-wrapper ul li a {
+  font-size: 17px;
+  line-height: 22px;
+  text-decoration: none;
+  color: white;
+  cursor: pointer;
+  border-bottom: 1px solid transparent;
+  padding-bottom: 3px;
+  transition: 0.3s;
+}
+nav.nav-wrapper ul li a:hover {
+  border-bottom: 1px solid white;
+}
+
+header .logo img {
+  max-width: 70px;
+}
+/*Navigation Bar styling  //  END*/
+
+
+/* ===========================================
+    PARALLAX PAGE STYLES  //  START
+   =========================================== */
+.cover-main {
+  background: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5)), url('../images/home-page-images/cool.gif') right bottom;
+  background-repeat: no-repeat;
+  background-size: cover;
+  height: 80vh;
+}
+ul#scene li.name {
+  text-align: center;
+  font-size: 40px;
+  line-height: 45px;
+  text-transform: uppercase;
+  font-weight: bold;
+  color: white;
+  width: 100%;
+}
+ul#scene li.title {
+  text-align: center;
+  font-size: 30px;
+  line-height: 35px;
+  color: white;
+  width: 100%;
+  margin-top: 60px;
+}
+ul#scene {
+  margin-top: 100px;
+}
+.btn {
+  width: 100%;
+  text-align: center;
+  margin-top: 150px;
+}
+.portfolio-btn,
+.contact-btn , #ct-btn{
+  margin: 0 auto;
+  display: inline-block;
+  padding: 10px 20px;
+  border: 1px solid white;
+  background: transparent;
+  color: white;
+  text-decoration: none;
+  transition: 0.3s;
+}
+.portfolio-btn {
+  margin-right: 30px;
+}
+.contact-btn, #ct-btn  {
+  margin-left: 30px;
+}
+.portfolio-btn:hover,
+.contact-btn:hover {
+  background: white;
+  color: black;
+  text-decoration: none;
+}
+/*PARALLAX PAGE STYLES END*/
+
+
+/* ===========================================
+    MASONRY JS STYLES  //  START
+   =========================================== */
+.masonry-grid {
+  position: relative;
+}
+.grid {
+  margin: 0 auto;
+}
+.grid-item {
+  width: 450px;
+  margin-bottom: 10px;
+}
+h2.sub-title {
+  position: relative;
+  display: block;
+  margin: 70px auto;
+  text-align: center;
+  font-size: 40px;
+  line-height: 40px;
+}
+h2.sub-title::after {
+  display: block;
+  height: 2px;
+  background-color: black;
+  content: "";
+  width: 100px;
+  margin: 30px auto 0 auto;
+}
+/*MASONRY JS STYLES END*/
+
+
+/* ===========================================
+    APP IMAGES STYLES  //  START
+   =========================================== */
+.app-image {
+  cursor: pointer;
+  position: relative;
+  overflow: hidden;
+  text-align:center;
+}
+.app-image .fadedbox {
+  background-color: #666666;
+  position: absolute;
+  top: 0;
+  left: 0;
+  color: #fff;
+  -webkit-transition: all 300ms ease-out;
+  -moz-transition: all 300ms ease-out;
+  -o-transition: all 300ms ease-out;
+  -ms-transition: all 300ms ease-out;
+  transition: all 300ms ease-out;
+  opacity: 0;
+  width: 450px;
+  height: 100px;
+  padding: 50px 20px 100px;
+}
+.app-image:hover .fadedbox { opacity: 0.8; }
+.app-image .text {
+  -webkit-transition: all 300ms ease-out;
+  -moz-transition: all 300ms ease-out;
+  -o-transition: all 300ms ease-out;
+  -ms-transition: all 300ms ease-out;
+  transition: all 300ms ease-out;
+  transform: translateY(30px);
+  -webkit-transform: translateY(30px);
+}
+.app-image .title {
+  font-size: 2.5em;
+  text-transform: uppercase;
+  opacity: 0;
+  transition-delay: 0.2s;
+  transition-duration: 0.3s;
+}
+.app-image:hover .title,
+.app-image:focus .title {
+  opacity: 1;
+  transform: translateY(0px);
+  -webkit-transform: translateY(0px);
+}
+/*APP IMAGE STYLE END*/
+
+
+/* ===========================================
+    I LOVE PYTHON STYLES  //  START
+   =========================================== */
+section.beating-heart {
+  background: linear-gradient(rgba(0,0,0,0.5), rgba(0,0,0,0.5)), url('../images/home-page-images/python-pic.png');
