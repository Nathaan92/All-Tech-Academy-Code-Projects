+                <span class="step-links">
+                    {% if parks_page_obj.has_previous%}
+                        <a href="{% my_url 1 'page' request.GET.urlencode %}">&laquo; First</a>
+                        <a href="{% my_url parks_page_obj.previous_page_number 'page' request.GET.urlencode %}">Previous</a>
