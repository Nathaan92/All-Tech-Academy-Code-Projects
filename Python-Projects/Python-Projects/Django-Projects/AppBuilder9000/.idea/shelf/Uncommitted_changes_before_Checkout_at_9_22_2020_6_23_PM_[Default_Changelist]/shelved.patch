+    for field in form.fields:
+        form.fields[field].disabled = True
+
+    return render(request, 'ApproachApp/GuidebookViewer.html', {'form': form, 'Guide': guide})
+
+
+def training(request):
+    return render(request, 'ApproachApp/Training.html')
